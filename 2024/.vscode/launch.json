{
	"version": "0.2.0",
	"configurations": [
			{
					"name": "debug-cmake",
					"type": "cppdbg",
					"request": "launch",
					"program": "${fileDirname}/build/ac2024",
					"args": [],
					"stopAtEntry": true,
					"cwd": "${workspaceFolder}",
					"environment": [],
					"externalConsole": false,
					"MIMode": "gdb",
					"setupCommands": [
							{
									"description": "Enable pretty-printing for gdb",
									"text": "-enable-pretty-printing",
									"ignoreFailures": true
							}
					],
					"preLaunchTask": "CMake: build",
					"miDebuggerPath": "/usr/bin/gdb"
			}
	]
}